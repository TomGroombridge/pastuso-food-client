{
  "extends": ["tslint:recommended", "tslint-react", "tslint-config-prettier"],
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts",
      "coverage/lcov-report/*.js"
    ]
  },
  "rules": {
    "no-console": false,
    "prefer-const": true,
    "jsx-no-lambda": false,
    "member-access": [false, "no-public"],
    "object-literal-sort-keys": false
  }
}
